-- –í —Å–∞–º–æ–º –Ω–∞—á–∞–ª–µ —Å–∫—Ä–∏–ø—Ç–∞ –¥–æ–±–∞–≤–ª—è–µ–º –∑–∞—â–∏—Ç—É –æ—Ç —Å–ª–∏—à–∫–æ–º —á–∞—Å—Ç—ã—Ö –∑–∞–ø—É—Å–∫–æ–≤:
if _G.SCRIPT_RUNNING_TIMESTAMP and os.time() - _G.SCRIPT_RUNNING_TIMESTAMP < 60 then
    print("‚ö†Ô∏è –°–ö–†–ò–ü–¢ –£–ñ–ï –ó–ê–ü–£–©–ï–ù! –ê–í–ê–†–ò–ô–ù–ê–Ø –û–°–¢–ê–ù–û–í–ö–ê...")
    _G.SCRIPT_RUNNING_TIMESTAMP = os.time()
    task.wait(5)
    game:GetService("TeleportService"):Teleport(85896571713843)
    return
end
_G.SCRIPT_RUNNING_TIMESTAMP = os.time()

-- [[ –º–µ–Ω—è–π —ç—Ç–∏ –Ω–∞—Å—Ç—Ä–æ–π–∫–∏ –¥–ª—è –æ–ø—Ç–∏–º–∏–∑–∞—Ü–∏–∏ ]]
getgenv().Optimization = {
    state = true, -- –≤–∫–ª/–≤—ã–∫–ª
    hide_screen = true, -- –≤—ã–∫–ª—é—á–∞–µ—Ç –ø—Ä–æ—Ä–∏—Å–æ–≤–∫—É –≥—Ä–∞—Ñ–∏–∫–∏, –±—É—Å—Ç–∏—Ç –∂–µ—Å—Ç–æ—á–∞–π—à–µ —Ñ–ø—Å (–≤–∫–ª/–≤—ã–∫–ª)
    fps = 10
};

-- –ù–∞—Å—Ç—Ä–æ–π–∫–∞ —Å–µ—Ä–≤–µ—Ä—Ö–æ–ø–∞
getgenv().ServerHopSettings = {
    fast_mode = true, -- —Ä–µ–∂–∏–º –±—ã—Å—Ç—Ä–æ–≥–æ —Å–µ—Ä–≤–µ—Ä—Ö–æ–ø–∞
    hop_interval = 15, -- –∏–Ω—Ç–µ—Ä–≤–∞–ª —Å–µ—Ä–≤–µ—Ä—Ö–æ–ø–∞ –≤ —Å–µ–∫—É–Ω–¥–∞—Ö
    message_interval = 3, -- –∏–Ω—Ç–µ—Ä–≤–∞–ª –º–µ–∂–¥—É —Å–æ–æ–±—â–µ–Ω–∏—è–º–∏ –≤ —Å–µ–∫—É–Ω–¥–∞—Ö
    messages_per_hop = 2 -- —Å–∫–æ–ª—å–∫–æ —Å–æ–æ–±—â–µ–Ω–∏–π –æ—Ç–ø—Ä–∞–≤–ª—è—Ç—å –Ω–∞ —Å–µ—Ä–≤–µ—Ä–µ
};

if getgenv().Optimization.state then
    if setfpscap ~= nil then setfpscap(getgenv().Optimization.fps); end
    UserSettings().GameSettings.MasterVolume = 0;
    settings().Rendering.QualityLevel = "Level01";
    game:GetService("RunService"):Set3dRenderingEnabled(not getgenv().Optimization.hide_screen);
end

local players = game:GetService("Players");
local local_player = players.LocalPlayer;
local http_service = game:GetService("HttpService");
local teleport_service = game:GetService("TeleportService");

-- –ù–û–í–´–ï –°–û–û–ë–©–ï–ù–ò–Ø –î–õ–Ø –û–¢–ü–†–ê–í–ö–ò
local chat_messages = {
    "Best Grow A Garden pet & sheckles shop ƒ£ƒ£/ astral-market",
    "Sheckles shop ƒ£ƒ£/ astral-market",
    "Beat the game with ƒ£ƒ£/ astral-market",
    "Giveaways happening at ƒ£ƒ£/ astral-market"
};

-- –ó–∞—â–∏—Ç–∞ –æ—Ç AFK –∫–∏–∫–∞
task.spawn(function()
    for _,v in pairs(getconnections(local_player.Idled)) do v:Disable() end
    for _,v in pairs(getconnections(game:GetService("UserInputService").WindowFocused)) do v:Disable() end
    for _,v in pairs(getconnections(game:GetService("UserInputService").WindowFocusReleased)) do v:Disable() end
end)

-- –§—É–Ω–∫—Ü–∏—è –æ—Ç–ø—Ä–∞–≤–∫–∏ —Å–æ–æ–±—â–µ–Ω–∏–π (–ø–æ–¥–¥–µ—Ä–∂–∫–∞ —Å—Ç–∞—Ä–æ–π –∏ –Ω–æ–≤–æ–π —Å–∏—Å—Ç–µ–º—ã —á–∞—Ç–∞)
local function sendMessage(message)
    local success = false
    
    -- –ü–æ–ø—ã—Ç–∫–∞ 1: –ò—Å–ø–æ–ª—å–∑–æ–≤–∞–Ω–∏–µ TextChatService (–Ω–æ–≤–∞—è —Å–∏—Å—Ç–µ–º–∞)
    pcall(function()
        local TextChatService = game:GetService("TextChatService")
        if TextChatService then
            local channel = TextChatService:FindFirstChild("TextChannels")
            if channel then
                local general = channel:FindFirstChild("RBXGeneral")
                if general then
                    general:SendAsync(message)
                    print("üìù –û—Ç–ø—Ä–∞–≤–ª–µ–Ω–æ —á–µ—Ä–µ–∑ TextChatService: " .. message)
                    success = true
                end
            end
        end
    end)
    
    -- –ü–æ–ø—ã—Ç–∫–∞ 2: –°—Ç–∞—Ä–∞—è —Å–∏—Å—Ç–µ–º–∞ —á–∞—Ç–∞, –µ—Å–ª–∏ –Ω–æ–≤–∞—è –Ω–µ —Å—Ä–∞–±–æ—Ç–∞–ª–∞
    if not success then
        pcall(function()
            local ReplicatedStorage = game:GetService("ReplicatedStorage")
            local ChatEvents = ReplicatedStorage:FindFirstChild("DefaultChatSystemChatEvents")
            if ChatEvents then
                local SayMessageRequest = ChatEvents:FindFirstChild("SayMessageRequest")
                if SayMessageRequest then
                    SayMessageRequest:FireServer(message, "All")
                    print("üìù –û—Ç–ø—Ä–∞–≤–ª–µ–Ω–æ —á–µ—Ä–µ–∑ Legacy Chat: " .. message)
                    success = true
                end
            end
        end)
    end
    
    -- –ü–æ–ø—ã—Ç–∫–∞ 3: –ü—Ä—è–º–æ–π –º–µ—Ç–æ–¥ –¥–ª—è –Ω–µ–∫–æ—Ç–æ—Ä—ã—Ö –∏–≥—Ä
    if not success then
        pcall(function()
            game:GetService("Players"):Chat(message)
            print("üìù –û—Ç–ø—Ä–∞–≤–ª–µ–Ω–æ —á–µ—Ä–µ–∑ Players:Chat: " .. message)
            success = true
        end)
    end
    
    return success
end

-- –£–ü–†–û–©–ï–ù–ù–ê–Ø –ò –ù–ê–î–ï–ñ–ù–ê–Ø –°–ò–°–¢–ï–ú–ê –°–ï–†–í–ï–†–•–û–ü–ê
local function serverHop()
    print("üîÑ –ù–∞—á–∏–Ω–∞–µ–º —Å–µ—Ä–≤–µ—Ä—Ö–æ–ø...")
    
    -- –ê–Ω–∫–æ—Ä–∏–º –ø–µ—Ä—Å–æ–Ω–∞–∂–∞ –¥–ª—è –ø—Ä–µ–¥–æ—Ç–≤—Ä–∞—â–µ–Ω–∏—è –∫—Ä–∞—à–µ–π
    pcall(function()
        if local_player.Character and local_player.Character:FindFirstChild("HumanoidRootPart") then
            local_player.Character.HumanoidRootPart.Anchored = true
        end
    end)
    
    -- –¶–µ–ª–µ–≤–∞—è –∏–≥—Ä–∞ (Bubble Gum Simulator)
    local placeId = 85896571713843
    
    -- –°–Ω–∞—á–∞–ª–∞ –ø—Ä–æ–±—É–µ–º –Ω–∞–π—Ç–∏ —Å–ª—É—á–∞–π–Ω—ã–π —Å–µ—Ä–≤–µ—Ä
    local serversUrl = string.format(
        "https://games.roblox.com/v1/games/%s/servers/Public?sortOrder=Asc&limit=100", 
        placeId
    )
    
    local servers
    pcall(function()
        servers = http_service:JSONDecode(game:HttpGet(serversUrl)).data
    end)
    
    -- –ï—Å–ª–∏ –Ω–∞—à–ª–∏ —Å–µ—Ä–≤–µ—Ä—ã, –≤—ã–±–∏—Ä–∞–µ–º –ø–æ–¥—Ö–æ–¥—è—â–∏–π
    if servers and #servers > 0 then
        -- –§–∏–ª—å—Ç—Ä—É–µ–º —Å–µ—Ä–≤–µ—Ä—ã (–Ω–µ –∑–∞—Ö–æ–¥–∏–º –Ω–∞ —Ç–µ–∫—É—â–∏–π)
        local filteredServers = {}
        for _, server in ipairs(servers) do
            if server.playing < 49 and server.id ~= game.JobId then
                table.insert(filteredServers, server)
            end
        end
        
        -- –ï—Å–ª–∏ –æ—Å—Ç–∞–ª–∏—Å—å –ø–æ–¥—Ö–æ–¥—è—â–∏–µ —Å–µ—Ä–≤–µ—Ä—ã
        if #filteredServers > 0 then
            -- –í—ã–±–∏—Ä–∞–µ–º —Å–ª—É—á–∞–π–Ω—ã–π —Å–µ—Ä–≤–µ—Ä
            local selectedServer = filteredServers[math.random(1, #filteredServers)]
            print("üéØ –ù–∞–π–¥–µ–Ω —Å–µ—Ä–≤–µ—Ä —Å " .. selectedServer.playing .. " –∏–≥—Ä–æ–∫–∞–º–∏. ID: " .. selectedServer.id)
            
            -- –¢–µ–ª–µ–ø–æ—Ä—Ç–∏—Ä—É–µ–º—Å—è
            teleport_service:TeleportToPlaceInstance(placeId, selectedServer.id, local_player)
            print("‚úàÔ∏è –¢–µ–ª–µ–ø–æ—Ä—Ç–∞—Ü–∏—è –Ω–∞ —Å–µ—Ä–≤–µ—Ä...")
            return true
        end
    end
    
    -- –ï—Å–ª–∏ –Ω–µ –Ω–∞—à–ª–∏ –ø–æ–¥—Ö–æ–¥—è—â–∏–µ —Å–µ—Ä–≤–µ—Ä—ã –∏–ª–∏ –ø—Ä–æ–∏–∑–æ—à–ª–∞ –æ—à–∏–±–∫–∞, –ø—Ä–æ—Å—Ç–æ —Ç–µ–ª–µ–ø–æ—Ä—Ç–∏—Ä—É–µ–º—Å—è –≤ –∏–≥—Ä—É
    print("‚ö†Ô∏è –ù–µ —É–¥–∞–ª–æ—Å—å –Ω–∞–π—Ç–∏ –ø–æ–¥—Ö–æ–¥—è—â–∏–µ —Å–µ—Ä–≤–µ—Ä—ã. –¢–µ–ª–µ–ø–æ—Ä—Ç–∞—Ü–∏—è –Ω–∞–ø—Ä—è–º—É—é...")
    teleport_service:Teleport(placeId, local_player)
    return true
end

-- –ù–û–í–ê–Ø –£–°–û–í–ï–†–®–ï–ù–°–¢–í–û–í–ê–ù–ù–ê–Ø –°–ò–°–¢–ï–ú–ê –°–ü–ê–ú–ê
-- –û—Ç–¥–µ–ª—å–Ω—ã–π –ø–æ—Ç–æ–∫ –¥–ª—è –æ—Ç–ø—Ä–∞–≤–∫–∏ —Å–æ–æ–±—â–µ–Ω–∏–π
task.spawn(function()
    -- –î–∞–µ–º –∏–≥—Ä–µ –≤—Ä–µ–º—è –∑–∞–≥—Ä—É–∑–∏—Ç—å—Å—è
    task.wait(3)
    
    print("üì£ –ó–∞–ø—É—â–µ–Ω–∞ —Å–∏—Å—Ç–µ–º–∞ —Å–ø–∞–º–∞ —Å–æ–æ–±—â–µ–Ω–∏–π")
    local messages_sent = 0
    local last_message = ""
    
    while true do
        -- –í—ã–±–∏—Ä–∞–µ–º —Å–ª—É—á–∞–π–Ω–æ–µ —Å–æ–æ–±—â–µ–Ω–∏–µ (–Ω–µ –ø–æ–≤—Ç–æ—Ä—è—è –ø—Ä–µ–¥—ã–¥—É—â–µ–µ)
        local message
        repeat
            message = chat_messages[math.random(1, #chat_messages)]
        until message ~= last_message or #chat_messages <= 1
        
        last_message = message
        
        -- –û—Ç–ø—Ä–∞–≤–ª—è–µ–º —Å–æ–æ–±—â–µ–Ω–∏–µ
        local success = sendMessage(message)
        
        if success then
            messages_sent = messages_sent + 1
            print("üì® –û—Ç–ø—Ä–∞–≤–ª–µ–Ω–æ —Å–æ–æ–±—â–µ–Ω–∏–µ #" .. messages_sent .. ": " .. message)
        end
        
        -- –ï—Å–ª–∏ –æ—Ç–ø—Ä–∞–≤–∏–ª–∏ –¥–æ—Å—Ç–∞—Ç–æ—á–Ω–æ —Å–æ–æ–±—â–µ–Ω–∏–π, –ø–µ—Ä–µ—Ö–æ–¥–∏–º –Ω–∞ –Ω–æ–≤—ã–π —Å–µ—Ä–≤–µ—Ä
        if messages_sent >= ServerHopSettings.messages_per_hop then
            print("üîÑ –û—Ç–ø—Ä–∞–≤–ª–µ–Ω–æ " .. messages_sent .. " —Å–æ–æ–±—â–µ–Ω–∏–π. –ù–∞—á–∏–Ω–∞–µ–º —Å–µ—Ä–≤–µ—Ä—Ö–æ–ø...")
            task.wait(1) -- –ù–µ–±–æ–ª—å—à–∞—è –ø–∞—É–∑–∞ –ø–µ—Ä–µ–¥ —Ö–æ–ø–æ–º
            serverHop()
            break -- –û—Å—Ç–∞–Ω–∞–≤–ª–∏–≤–∞–µ–º —Ü–∏–∫–ª, —Ç–∞–∫ –∫–∞–∫ –±—É–¥–µ–º –ø–µ—Ä–µ—Ö–æ–¥–∏—Ç—å –Ω–∞ –¥—Ä—É–≥–æ–π —Å–µ—Ä–≤–µ—Ä
        end
        
        -- –û–∂–∏–¥–∞–Ω–∏–µ –ø–µ—Ä–µ–¥ —Å–ª–µ–¥—É—é—â–∏–º —Å–æ–æ–±—â–µ–Ω–∏–µ–º
        task.wait(ServerHopSettings.message_interval)
    end
end)

-- –†–ï–ó–ï–†–í–ù–ê–Ø –°–ò–°–¢–ï–ú–ê –°–ï–†–í–ï–†–•–û–ü–ê
-- –¢–∞–π–º–µ—Ä –¥–ª—è –ø–µ—Ä–µ—Ö–æ–¥–∞ –Ω–∞ –Ω–æ–≤—ã–π —Å–µ—Ä–≤–µ—Ä, –µ—Å–ª–∏ —á—Ç–æ-—Ç–æ –ø–æ—à–ª–æ –Ω–µ —Ç–∞–∫
task.spawn(function()
    -- –ñ–¥–µ–º –º–∞–∫—Å–∏–º–∞–ª—å–Ω—ã–π —Ç–∞–π–º–∞—É—Ç –Ω–∞ —Å–µ—Ä–≤–µ—Ä–µ
    local max_time = ServerHopSettings.hop_interval
    print("‚è±Ô∏è –ú–∞–∫—Å–∏–º–∞–ª—å–Ω–æ–µ –≤—Ä–µ–º—è –Ω–∞ —Å–µ—Ä–≤–µ—Ä–µ: " .. max_time .. " —Å–µ–∫—É–Ω–¥")
    
    task.wait(max_time)
    
    print("‚è∞ –¢–∞–π–º–∞—É—Ç –∏—Å—Ç–µ–∫! –ü—Ä–∏–Ω—É–¥–∏—Ç–µ–ª—å–Ω—ã–π —Å–µ—Ä–≤–µ—Ä—Ö–æ–ø...")
    serverHop()
end)

-- –û–±—Ä–∞–±–æ—Ç–∫–∞ –∫–∏–∫–∞ —Å —Å–µ—Ä–≤–µ—Ä–∞
game:GetService("CoreGui"):FindFirstChild("RobloxPromptGui"):FindFirstChild("promptOverlay").ChildAdded:Connect(function(child)
    if child.Name == "ErrorPrompt" then
        print("‚ùå –û–±–Ω–∞—Ä—É–∂–µ–Ω –∫–∏–∫ —Å —Å–µ—Ä–≤–µ—Ä–∞. –ü–µ—Ä–µ—Ö–æ–¥–∏–º –Ω–∞ –Ω–æ–≤—ã–π —Å–µ—Ä–≤–µ—Ä...")
        serverHop()
    end
end)

-- –û–±—Ä–∞–±–æ—Ç–∫–∞ –æ—à–∏–±–æ–∫ —Ç–µ–ª–µ–ø–æ—Ä—Ç–∞—Ü–∏–∏
local_player.OnTeleport:Connect(function(state)
    if state == Enum.TeleportState.Failed then
        print("‚ùå –¢–µ–ª–µ–ø–æ—Ä—Ç–∞—Ü–∏—è –Ω–µ —É–¥–∞–ª–∞—Å—å! –ü–æ–≤—Ç–æ—Ä–Ω–∞—è –ø–æ–ø—ã—Ç–∫–∞...")
        task.wait(3)
        serverHop()
    end
end)

print("‚úÖ –°–∫—Ä–∏–ø—Ç —É—Å–ø–µ—à–Ω–æ –∑–∞–ø—É—â–µ–Ω!")
